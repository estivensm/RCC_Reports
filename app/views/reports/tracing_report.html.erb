<p id="notice"><%= notice %></p>

<h1>Tracings</h1>

<table>
  <thead>
    <tr>
      <th>Attachment</th>
      <th>Description</th>
      <th>Reports</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seguimientos.each do |seguimiento| %>
      <tr>
        <td><%= seguimiento.attachment %></td>
        <td><%= seguimiento.description %></td>
        <td><%= seguimiento.report %></td>
        <td><%= link_to 'Show', seguimiento %></td>
        <td><%= link_to 'Edit', edit_seguimiento_path(seguimiento) %></td>
        <td><%= link_to 'Destroy', seguimiento, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tracing', new_seguimiento_path %>



<%= form_tag create_tracing_path, html: { multipart: true } do %>  
<div class="form-group">
  <%= hidden_field_tag :report_id, @report.id %>  
  <%= label_tag :attachment %>  
  <%= file_field_tag 'attachment' ,:class => 'form form-control' %>  
    </div>
    <div class="form-group">

  <%= label_tag :description %>  
  <%= text_area_tag :description ,nil, :class => 'form form-control' %>  
    </div>
  <%= submit_tag "Forward", :class => 'btn btn-primary'%>    
<% end %>  


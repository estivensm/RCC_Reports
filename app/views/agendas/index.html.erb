<p id="notice"><%= notice %></p>

<h1>Agendas</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Empresa</th>
      <th>Cargo</th>
      <th>Telefono</th>
      <th>Celular</th>
      <th>Email</th>
      <th>Direccion empresa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agendas.each do |agenda| %>
      <tr>
        <td><%= agenda.nombre %></td>
        <td><%= agenda.empresa %></td>
        <td><%= agenda.cargo %></td>
        <td><%= agenda.telefono %></td>
        <td><%= agenda.celular %></td>
        <td><%= agenda.email %></td>
        <td><%= agenda.direccion_empresa %></td>
        <td><%= link_to 'Show', agenda %></td>
        <td><%= link_to 'Edit', edit_agenda_path(agenda) %></td>
        <td><%= link_to 'Destroy', agenda, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

